{
  "name": "obsidian-wikivault",
  "version": "1.1.0",
  "description": "Automatically generates Wikipedia-like notes for missing Wikilinks with context extraction and AI summaries. Now with LM Studio support!",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "wiki",
    "wikilinks",
    "ai",
    "lm-studio",
    "local-ai"
  ],
  "author": "Manus",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
