  "includeAliases": true,
  "alwaysShowMultipleReferences": true,
  "minWordLengthForAutoDetect": 3,
  "maxWordsToMatch": 3,
  "preferLongerMatches": true,
  "showAllPossibleMatches": true,
  "excludedFileTypes": [
